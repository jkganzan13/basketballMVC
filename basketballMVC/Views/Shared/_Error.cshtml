<!--Success/Error Popup-->
@if (TempData["Error"] != null)
{
    <div class="alert alert-danger">
        <a href="#" class="close" data-dismiss="alert">&times;</a>
        <span class="glyphicon glyphicon-warning-sign"></span> <strong>Error!</strong> @Html.Raw(TempData["Error"])
</div>
}
@if (TempData["Success"] != null)
{
    <div class="alert alert-success">
        <a href="#" class="close" data-dismiss="alert">&times;</a>
        <span class="glyphicon glyphicon-ok-sign"></span> <strong>Success!</strong> @Html.Raw(TempData["Success"])
    </div>
}
<!--End Popup-->
